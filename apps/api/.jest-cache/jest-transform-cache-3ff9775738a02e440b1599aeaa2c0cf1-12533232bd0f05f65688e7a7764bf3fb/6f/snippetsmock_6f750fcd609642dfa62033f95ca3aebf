/**
 * Erstellt einen Mock Snippet
 */
function createMockSnippet(overrides = {}) {
    const now = new Date();
    return {
        id: 'snippet-test-id-123',
        title: 'Test Snippet',
        description: 'A test snippet for unit tests',
        code: 'console.log("Hello World")',
        language: 'typescript',
        userId: 'user-test-id-123',
        isPublic: true,
        viewCount: 0,
        createdAt: now,
        updatedAt: now,
        ...overrides,
    };
}
/**
 * Erstellt einen Mock Snippet Preview (ohne Code)
 */
function createMockSnippetPreview(overrides = {}) {
    // eslint-disable-next-line @typescript-eslint/no-unused-vars
    const { code, ...preview } = createMockSnippet(overrides);
    return preview;
}
/**
 * Erstellt mehrere Mock Snippets
 */
function createMockSnippets(count, baseOverrides = {}) {
    return Array.from({ length: count }, (_, i) => createMockSnippet({
        ...baseOverrides,
        id: `snippet-${i}`,
        title: `Test Snippet ${i + 1}`,
    }));
}
