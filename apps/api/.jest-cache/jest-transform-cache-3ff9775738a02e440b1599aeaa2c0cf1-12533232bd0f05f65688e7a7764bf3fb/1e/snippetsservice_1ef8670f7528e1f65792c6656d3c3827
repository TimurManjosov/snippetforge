  /**
   * Validiert Snippet-Daten (Business Rules)
   *
   * WICHTIG: Zod validiert bereits Format (z.B. min/max length)
   * Diese Methode prft zustzliche Business Rules
   *
   * @param data - Snippet-Daten
   * @throws BadRequestException bei ungltigen Daten
   */
  validateSnippetData(data) {
    /* istanbul ignore next */
    cov_6wu4bgvsx().f[18]++;
    cov_6wu4bgvsx().s[92]++;
    // Code darf nicht nur Whitespace sein
    if (data.code !== undefined) {
      /* istanbul ignore next */
      cov_6wu4bgvsx().b[46][0]++;
      const trimmedCode =
      /* istanbul ignore next */
      (cov_6wu4bgvsx().s[93]++, data.code.trim());
      /* istanbul ignore next */
      cov_6wu4bgvsx().s[94]++;
      if (trimmedCode.length === 0) {
        /* istanbul ignore next */
        cov_6wu4bgvsx().b[47][0]++;
        cov_6wu4bgvsx().s[95]++;
        throw new common_1.BadRequestException('Code cannot be empty or contain only whitespace');
      } else
      /* istanbul ignore next */
      {
        cov_6wu4bgvsx().b[47][1]++;
      }
      // Code max 50,000 Zeichen (Business Rule)
      cov_6wu4bgvsx().s[96]++;
      if (trimmedCode.length > 50000) {
        /* istanbul ignore next */
        cov_6wu4bgvsx().b[48][0]++;
        cov_6wu4bgvsx().s[97]++;
        throw new common_1.BadRequestException('Code exceeds maximum length of 50,000 characters');
      } else
      /* istanbul ignore next */
      {
        cov_6wu4bgvsx().b[48][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_6wu4bgvsx().b[46][1]++;
    }
    // Title darf nicht nur Whitespace sein
    cov_6wu4bgvsx().s[98]++;
    if (data.title !== undefined) {
      /* istanbul ignore next */
      cov_6wu4bgvsx().b[49][0]++;
      const trimmedTitle =
      /* istanbul ignore next */
      (cov_6wu4bgvsx().s[99]++, data.title.trim());
      /* istanbul ignore next */
      cov_6wu4bgvsx().s[100]++;
      if (trimmedTitle.length === 0) {
        /* istanbul ignore next */
        cov_6wu4bgvsx().b[50][0]++;
        cov_6wu4bgvsx().s[101]++;
        throw new common_1.BadRequestException('Title cannot be empty or contain only whitespace');
      } else
      /* istanbul ignore next */
      {
        cov_6wu4bgvsx().b[50][1]++;
      }
      // Title max 200 Zeichen (Business Rule)
      cov_6wu4bgvsx().s[102]++;
      if (trimmedTitle.length > 200) {
        /* istanbul ignore next */
        cov_6wu4bgvsx().b[51][0]++;
        cov_6wu4bgvsx().s[103]++;
        throw new common_1.BadRequestException('Title exceeds maximum length of 200 characters');
      } else
      /* istanbul ignore next */
      {
        cov_6wu4bgvsx().b[51][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_6wu4bgvsx().b[49][1]++;
    }
    // Language darf nicht leer sein
    cov_6wu4bgvsx().s[104]++;
    if (data.language !== undefined) {
      /* istanbul ignore next */
      cov_6wu4bgvsx().b[52][0]++;
      const trimmedLanguage =
      /* istanbul ignore next */
      (cov_6wu4bgvsx().s[105]++, data.language.trim());
      /* istanbul ignore next */
      cov_6wu4bgvsx().s[106]++;
      if (trimmedLanguage.length === 0) {
        /* istanbul ignore next */
        cov_6wu4bgvsx().b[53][0]++;
        cov_6wu4bgvsx().s[107]++;
        throw new common_1.BadRequestException('Language cannot be empty');
      } else
      /* istanbul ignore next */
      {
        cov_6wu4bgvsx().b[53][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_6wu4bgvsx().b[52][1]++;
    }
  }
