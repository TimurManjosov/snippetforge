    return exports.UpdateSnippetSchema.parse(data);
}
/**
 * Safe Validation (wirft keinen Error)
 * Gibt Result-Object zurück
 */
function safeValidateUpdateSnippetDto(data) {
    return exports.UpdateSnippetSchema.safeParse(data);
